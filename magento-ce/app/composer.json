{
  "require": {
    "php": ">=5.3.0",
    "aydin-hassan/magento-core-composer-installer": "^1.0",
    "inviqa/magento-community": "1.9.2.4",
    "n98/magerun": "*"
  },
  "repositories": [
    {"type": "vcs", "url": "git@github.com:inviqa/magento-community.git"}
  ],
  "config": {
    "bin-dir": "bin",
    "discard-changes": "stash"
  },
  "extra": {
    "magento-root-dir": "public",
    "magento-deploystrategy": "copy",
    "magento-force": true
  }
}
